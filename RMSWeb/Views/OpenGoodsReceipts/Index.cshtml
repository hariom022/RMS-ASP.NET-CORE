@model List<GoodsReceiptOverview>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}


<div class="col-12">
    <div class="card good-Receipt overflow-auto">
        <div class="card-body">
            <h5 class="card-title">Open Good Receipts Report</h5>

            <div class="row d-flex justify-content-between">
                <div class="col-md-4 mb-3">
                    <label for="startDate" class="form-label">Start Date:</label>
                    <input type="date" class="form-control" id="startDate" />
                </div>
                <div class="col-md-4 mb-3">
                    <label for="endDate" class="form-label">End Date:</label>
                    <input type="date" class="form-control" id="endDate" />
                </div>
            </div>
            <div style="width: 100%; overflow-x: auto">
                <table class="table table-hover" style="overflow-x: auto">
                    <thead>
                        <tr class="text-center">
                            <th scope="col">Request No</th>
                            <th scope="col">Quotation No</th>
                            <th scope="col">Sales No</th>
                            <th scope="col">OBD No</th>
                            <th scope="col">GoodReceipt</th>
                            
                        </tr>
                    </thead>
                    <tbody>
                        @foreach(var item in Model)
                        {
                            <tr class="text-center">
                                <td>@item.RequestNo</td>
                                <td>@item.QuotationNo</td>
                                <td>@item.SalesNo</td>
                                <td>@item.OBDNo</td>
                                <td>@item.GoodReceipt</td>
                              
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>