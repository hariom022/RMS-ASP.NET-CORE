@model List<LastConsumptionReport>

<div class="col-12">
    <div class="card good-Receipt overflow-auto">
        <div class="card-body">
            <h5 class="card-title">Consumption Report</h5>
            <div style="width: 100%; overflow-x: auto">
                <table class="table table-hover">
                    <thead class="table-dark">
                        <tr class="text-center">
                            <th>Medicine Name</th>
                            <th>Quantity Consumed</th>
                            <th>Date Consumed</th>
                            <th>Department</th>
                        </tr>
                    </thead>
                    <tbody>
                        <!-- Example data, replace with actual data from your database -->
                        @foreach(var item in Model)
                        {
                            <tr class="text-center">
                                <td>@item.MedicineName</td>
                                <td>@item.QuantityConsumed</td>
                                <td>@item.DateConsumed.ToString("dd/MM/yyyy")</td>
                                <td>@item.Department</td>
                            </tr>
                        }
                       
                      
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
