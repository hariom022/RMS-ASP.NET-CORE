@{
}
<div class="col-12">
    <div class="card good-Receipt overflow-auto">
        <div class="card-body">
            <h5 class="card-title">Template Upload Page</h5>

            <form enctype="multipart/form-data">
                <div class="mb-3">
                    <label for="templateFile" class="form-label">Select Template File</label>
                    <input type="file"
                           class="form-control"
                           id="templateFile"
                           name="templateFile"
                           required />
                </div>
                <button type="submit" class="btn btn-primary">Upload</button>
            </form>
        </div>
    </div>
    <div class="btn-container text-center">
        <a asp-controller="InventoryUpdateOverview" asp-action="index">
            <button class="bg-secondary text-light p-2" style="border: none">
                Back
            </button>
        </a>
    </div>
</div>