@model RMS.Models.ProductMasterOverview

@{
    ViewData["Title"] = "Details";
}







<div class="col-12 mt-5">

    <div class="card good-Receipt overflow-auto">
        <div class="card-body">
            <h5 class="card-title">Product Material Details</h5>
            <div>
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ProductId)
                    </dt>
                    <dd class="col-sm-10">
                        :  @Html.DisplayFor(model => model.ProductId)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ProductNumber)
                    </dt>
                    <dd class="col-sm-10">
                        : @Html.DisplayFor(model => model.ProductNumber)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ProductDescription)
                    </dt>
                    <dd class="col-sm-10">
                        :@Html.DisplayFor(model => model.ProductDescription)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ProductGroup)
                    </dt>
                    <dd class="col-sm-10">
                        : @Html.DisplayFor(model => model.ProductGroup)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.UnitOfMeasurement)
                    </dt>
                    <dd class="col-sm-10">
                        : @Html.DisplayFor(model => model.UnitOfMeasurement)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ProductType)
                    </dt>
                    <dd class="col-sm-10">
